<h1>New Users</h1>


<form action="<%= users_url(@user) %>" method="post">

  <label for="user_name">User Name</label>
  <input
    type="text"
    name="user[user_name]"
    id="user_name"
    value="<%= @user.user_name %>">

  <label for="password">Password</label>
  <input
    type="text"
    name="user[password]"
    id="password"
    value="<%= @user.password %>">
  <br>

  <input type="submit" value="--Create User--">
</form>
