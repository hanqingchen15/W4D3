<h1>Login Page</h1>

<form action="<%= session_url %>" method="post">


  <label for="user_name">User Name</label>
  <input
    type="text"
    name="user[user_name]"
    id="user_name">

  <label for="password">Password</label>
  <input
    type="text"
    name="user[password]"
    id="password">
  <br>

  <input type="submit" value="--Login--">
</form>